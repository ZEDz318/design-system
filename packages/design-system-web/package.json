{
  "name": "@geovistory/design-system-web",
  "version": "1.11.1-pr-69.0",
  "description": "geovistory.org's design system: web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-web/design-system-web.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geovistory/design-system.git"
  },
  "keywords": [
    "geovistory",
    "design-system",
    "web-components"
  ],
  "author": "geovistory.org",
  "bugs": {
    "url": "https://github.com/geovistory/design-system/issues"
  },
  "homepage": "",
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "start": "npm run start:storybook",
    "=========================== DEV ==========================": "",
    "dev": "concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm run dev:stencil\" \"npm run sleep5s && npm run dev:storybook\"",
    "dev:stencil": "stencil build --config stencil.dev.config.ts --watch",
    "dev:storybook": "storybook dev -p 6006 --quiet",
    "dev:stencil-only": "npx stencil build --dev --watch --serve --config stencil.dev.config.ts",
    "sleep5s": "node -e \"setTimeout(() => process.exit(0), 10000)\"",
    "=========================== STENCIL ==========================": "",
    "generate": "npx stencil generate",
    "==================== BUILD ====================": "================================",
    "build": "npx stencil build && node .build/stencil.post-build.js && rollup -c rollup.config.js",
    "build:docs": "npm run build:stencil && storybook build -o ../../storybook  --quiet",
    "build:stencil": "npx stencil build --docs --config stencil.dev.config.ts && node .build/stencil.post-build.js",
    "build-watch:stencil": "npx stencil build --watch",
    "build:storybook": "storybook build",
    "==================== TEST =====================": "================================",
    "test": "npm run test:stencil",
    "test:stencil": "npx stencil build --config stencil.test.config.ts && npx stencil test --spec",
    "test-watch:stencil": "npx stencil test --spec --e2e --watchAll",
    "========================== STORYBOOK =========================": "",
    "start:storybook": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^3.3.0",
    "patch-package": "^6.4.7",
    "prismjs": "^1.29.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-icons": "^4.3.1",
    "react-syntax-highlighter": "^15.5.0",
    "swiper": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@ionic/core": "6.0.14",
    "@prettier/plugin-xml": "^3.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@stencil/angular-output-target": "^0.5.0",
    "@stencil/react-output-target": "^0.4.0",
    "@stencil/sass": "^2.0.3",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/html-vite": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.0.24",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/web-components-vite": "^7.0.24",
    "@types/jest": "^27.0.3",
    "@types/plotly.js": "2.12.18",
    "@types/prismjs": "^1.26.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "ionicons": "^6.0.2",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "storybook": "^7.0.24"
  },
